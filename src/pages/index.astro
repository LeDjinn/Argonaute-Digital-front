---
import Layout from "@layouts/Default.astro";
import RecentBlogPosts from "@components/generic/RecentBlogPosts.astro";
import MobileSocials from "@components/home/MobileSocials.astro";
import { Pill } from "@eliancodes/brutal-ui";
import { Button } from "@eliancodes/brutal-ui";
import  Card  from "@components/original components/Card.astro";
import Swiper from "@components/test component/Swiper.astro";

const builtWith = [
  { name: "Astro", url: "https://astro.build/" },
  { name: "UnoCSS", url: "https://uno.antfu.me/" },
  {
    name: "Content Collections",
    url: "https://docs.astro.build/en/guides/content-collections/",
  },
  { name: "RSS", url: "https://docs.astro.build/en/guides/rss/" },
  {
    name: "Sitemap",
    url: "https://docs.astro.build/en/guides/integrations-guide/sitemap/",
  },
  { name: "Vercel", url: "https://vercel.com" },
  { name: "TypeScript", url: "https://www.typescriptlang.org/" },
];
const benefits = [
  {
    title: "Innovative Solutions",
    body: "Stay ahead of the curve with our forward-thinking approach.",
  },
  {
    title: "Collaborative Partnership",
    body: "Your vision, our expertise — a partnership for success.",
  },
  {
    title: "Results-Driven Focus",
    body: "We don't just build, we deliver measurable results.",
  },
];
const services = [
  {
    title: "Applications Tailored to Improve Your Workflow",
    image: "https://argonaute-digital.com/images/assets/blue.png",
    body: "Discover custom applications designed to enhance your workflow. At Argonaute Digital, we specialize in crafting bespoke solutions that align perfectly with your business needs. From streamlining processes to boosting efficiency, our applications are made to measure, ensuring a seamless integration into your daily operations.",
  },
  {
    title: "Solve Your Business Challenges with Software",
    image: "https://argonaute-digital.com/images/assets/violet.png",
    body: "Navigate the complexities of your business challenges with our powerful software solutions. Argonaute Digital is dedicated to providing innovative answers to your unique problems. Our tailored software solutions are crafted to address your specific pain points, enabling you to overcome obstacles and drive success in your industry.",
  },
  {
    title: "Build Custom Websites and Web Apps",
    image: "https://argonaute-digital.com/images/assets/pink.png",
    body: " Transform your online presence with custom websites and web applications. Argonaute Digital is your partner in building digital experiences that stand out. Whether you need a captivating website or a dynamic web application, our team combines creativity and technical expertise to bring your vision to life. Elevate your brand and engage your audience with our custom web solutions.",
  },
];
---

<Layout
  title="Home"
  pageTitle="Argonaute Dgital | Home"
  description="Empowering businesses with cutting-edge software solutions."
>
  <main class="p-6">
    <MobileSocials />
    <section id="about" class="grid md:grid-cols-8 gap-8 mt-4">
      <h1 class="hidden">Argonaute Digital</h1>
      <div class="col-span-6">
        <Card>
          <div class="flex flex-col justify-between items-start gap-4">
            <p class="mt-4 outfit text-2xl md:text-5xl lg:text-7xl">
              Welcome to <span class="text-white">Argonaute Digital</span>
            </p>
            <p class="mt-2 outfit text-xl md:text-3xl lg:text-5xl">
              Empowering businesses with cutting-edge software solutions.
            </p>
            <Button target={"_self"} href="/blog/">
              Explore solutions &rarr;
            </Button>
          </div>
        </Card>
        <div class="grid md:grid-cols-8 gap-8 mt-4">
          <div class="col-span-3">
            <Card>
              <h2 class="text-2xl md:text-4xl lg:text-6xl mb-8 dm-serif">
                Elevating Your Results
              </h2>
              <p class="mt-2 outfit text-xl md:text-xl lg:text-2xl">
                We're not just developers. <br /> We're your architects of
                possibility, crafting solutions that save you time and money.
              </p>
            </Card>
          </div>
          <div class="col-span-5">
            <Card>
              <h2 class="text-2xl md:text-4xl lg:text-6xl mb-8 dm-serif">
                We bring you :
              </h2>

              {
                benefits.map((benefit) => (
                  <div class="mb-8">
                    <h3 class="text-xl font-semibold mb-2">{benefit.title}</h3>
                    <Pill>{benefit.body}</Pill>
                  </div>
                ))
              }
            </Card>
          </div>
        </div>
      </div>
      <div class="col-span-2">
        <img
          src="https://new-alerts-e4f6j5kdsq-ew.a.run.app/rails/active_storage/blobs/redirect/eyJfcmFpbHMiOnsibWVzc2FnZSI6IkJBaHBKZz09IiwiZXhwIjpudWxsLCJwdXIiOiJibG9iX2lkIn19--56132449c17237bff62a8b5d03e5c662ee637cc2/hero.png"
          class="object-cover h-full w-full rounded-lg md:col-span-2 border-3 border-black card-shadow"
          alt="Elian Van Cutsem"
          width="392"
          height="392"
          loading="lazy"
          decoding="async"
        />
      </div>
    </section>
 
    <section class="mt-8">
      <Card>
        <h2 class="text-2xl md:text-4xl lg:text-6xl mb-8 dm-serif">
          Our Core Strengths :
        </h2>
        <div class="grid md:grid-cols-9 gap-8 mt-4">
          {
            services.map((item) => (
              <div class="col-span-3 ">
                <Card>
                  <h3 class="text-2xl outfit  font-semibold mb-2">
                    {item.title}
                  </h3>
                  <div class="flex flex-col md:flex-row  justify-around items-center">
                    <img src={item.image} alt={item.title} width="256" />
                    <p class="mt-2 p-2 md:p-4 text-white outfit text-xl md:text-l lg:text-xl">
                      {item.body}
                    </p>
                  </div>
                </Card>
              </div>
            ))
          }
        </div>
      </Card>
    </section>
    <section class="mt-8">
      <Card color="white ">

 
  
          <h2 class="text-2xl text-center md:text-4xl lg:text-6xl mb-8 dm-serif">
            Let's turn your ideas into impactful solutions. Contact us now, and let's craft the future of your online success together
          </h2>
          <div class="flex items-center justify-center ">
  
            <Button target={"_self"} href="/contact/" >Get in touch &rarr;</Button>
          </div>
   
      </Card>
      

 
   </section>
  
    <section class="mt-8">
      <Card>
        <h2 class="text-2xl md:text-4xl lg:text-6xl mb-8 dm-serif">
          They trust us
        </h2>
        <Swiper/>
       
      </Card>
    </section>
    <section class="mt-8">
      <Card>
      <div class="flex justify-around">
 
        <img src="https://argonaute-digital.com/images/assets/robotlogo blue.svg" width="256px"  alt="logo">
        <img src="https://argonaute-digital.com/images/assets/robotlogo pink.svg" width="256px"  alt="logo">
        <img src="https://argonaute-digital.com/images/assets/robotlogo yellow.svg" width="256px"  alt="logo">
      </div>
      </Card>
    </section>
    <RecentBlogPosts />
  </main>
</Layout>
<style>
  .red {
    background-color: red;
  }
  .blue {
    background-color: blue;
  }
  green {
    background-color: green;
  }
</style>
